<!DOCTYPE html>
<html>
<head>
  <title>Matchme</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <%= form_tag '/logout', method: 'get' do %>
      <%= submit_tag "logout" %>
    <% end %>
  <% end %>


<%= yield %>

</body>
</html>
